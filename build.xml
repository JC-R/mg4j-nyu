  </path>
  
  <path id="mg4j-nyu.module.production.classpath">
    <path refid="${module.jdk.classpath.mg4j-nyu}"/>
    <path refid="library.jars.classpath"/>
  </path>
  
  <path id="mg4j-nyu.runtime.production.module.classpath">
    <pathelement location="${mg4j-nyu.output.dir}"/>
    <path refid="library.jars.classpath"/>
  </path>
  
  <path id="mg4j-nyu.module.classpath">
    <path refid="${module.jdk.classpath.mg4j-nyu}"/>
    <pathelement location="${mg4j-nyu.output.dir}"/>
    <path refid="library.jars.classpath"/>
  </path>
  
  <path id="mg4j-nyu.runtime.module.classpath">
    <pathelement location="${mg4j-nyu.testoutput.dir}"/>
    <pathelement location="${mg4j-nyu.output.dir}"/>
    <path refid="library.jars.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.mg4j-nyu">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.mg4j-nyu">
    <patternset refid="excluded.from.module.mg4j-nyu"/>
  </patternset>
  
  <path id="mg4j-nyu.module.sourcepath">
    <dirset dir="${module.mg4j-nyu.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.mg4j-nyu" depends="compile.module.mg4j-nyu.production,compile.module.mg4j-nyu.tests" description="Compile module mg4j-nyu"/>
  
  <target name="compile.module.mg4j-nyu.production" description="Compile module mg4j-nyu; production classes">
    <mkdir dir="${mg4j-nyu.output.dir}"/>
    <javac destdir="${mg4j-nyu.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.mg4j-nyu}/javac">
      <compilerarg line="${compiler.args.mg4j-nyu}"/>
      <bootclasspath refid="mg4j-nyu.module.bootclasspath"/>
      <classpath refid="mg4j-nyu.module.production.classpath"/>
      <src refid="mg4j-nyu.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.mg4j-nyu"/>
    </javac>
    
    <copy todir="${mg4j-nyu.output.dir}">
      <fileset dir="${module.mg4j-nyu.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.mg4j-nyu.tests" depends="compile.module.mg4j-nyu.production" description="compile module mg4j-nyu; test classes" unless="skip.tests"/>
  
  <target name="clean.module.mg4j-nyu" description="cleanup module">
    <delete dir="${mg4j-nyu.output.dir}"/>
    <delete dir="${mg4j-nyu.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.mg4j-nyu" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.mg4j-nyu" description="build all modules"/>
  
